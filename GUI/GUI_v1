from tkinter import Tk, Label
print("imported")

root = Tk() #el window box 
#has to go first





#label widget
label1 = Label(root,text="Hello World!") #
label2 = Label(root,text="" \
"EEEEEEEEEEEEEEEE") #



#putting it on the screen using grids
#rows and columns 
label1.grid(row=0,column=0)
label2.grid(row=1,column=1)


root.mainloop()